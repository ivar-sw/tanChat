{
  "name": "tanchat",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite dev\" \"tsx watch --env-file .env src/server/ws.ts\"",
    "dev:web": "vite dev",
    "dev:ws": "tsx watch --env-file .env src/server/ws.ts",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "start:ws": "tsx --env-file .env src/server/ws.ts",
    "start:all": "concurrently \"pnpm start\" \"pnpm start:ws\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:cleanup:e2e": "node scripts/cleanup-e2e-data.js",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@libsql/client": "^0.17.0",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.4.0",
    "@tanstack/react-router": "^1.161.1",
    "@tanstack/react-start": "^1.161.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^1.1.1",
    "drizzle-orm": "^0.38.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@eslint-react/eslint-plugin": "^2.12.4",
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.5.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "jsdom": "^28.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild"
    ]
  }
}
